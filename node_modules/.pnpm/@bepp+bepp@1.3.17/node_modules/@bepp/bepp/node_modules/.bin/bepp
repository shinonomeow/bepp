#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules/@bepp/bepp/bin/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules/@bepp/bepp/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules/@bepp/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules/@bepp/bepp/bin/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules/@bepp/bepp/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules/@bepp/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/@bepp+bepp@1.3.17/node_modules:/Users/shino/my_code/github/bepp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/main.js" "$@"
else
  exec node  "$basedir/../../bin/main.js" "$@"
fi
